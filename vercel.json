{
  "buildCommand": "echo 'No build'",
  "outputDirectory": ".",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/vendor-qrcode.min.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ],
  "scripts": [
    {
      "src": "./main.js"
    },
    {
      "src": "./vendor-qrcode.min.js"
    }
  ]
}